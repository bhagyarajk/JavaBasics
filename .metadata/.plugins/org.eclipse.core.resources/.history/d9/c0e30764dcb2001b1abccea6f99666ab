package com.keepitsimple.java;

public class AnonymousInnerClasses {

	public static void main(String[] args) {
		Runnable runnable = new Runnable() {

			@Override
			public void run() {
				// TODO Auto-generated method stub
				for (int i = 0; i < 10; i++) {
					System.out.println("Thread-2 method ..");
					
				}

			}
		}

		;
	}
}
