package com.keepitsimple.cloning;

public class ShallowCloningDemo {
public static void main(String[] args) {
	
}

}

// step 1 : In order to make the Demo1 class clonable, implement the clonable interface first 
class Demo1 implements Cloneable {
	int a;
	int b;
	Demo2 demo2;

	// step 2 : Override the clone method : even though the cloneable is the marker
	// interface, within this method return the super.clone() and it's called the
	// shallow cloning
	@Override
	public Object clone() throws CloneNotSupportedException {
		return super.clone();

	}
}

class Demo2 {
	int x;
	int y;

}
